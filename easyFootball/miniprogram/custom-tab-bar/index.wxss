@font-face {
  font-family: 'test';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}





.tabbar_box {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  height: 120rpx;
  box-shadow: 0 0 3rpx rgba(0, 0, 0, 0.2);
}

.tabbar_nav {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  font-family: 'test';
  height: 100%;
  position: relative;
  z-index: 999;
  /* background-color: #fff; */
}

.tabbar_icon {
  width: 64rpx;
  height: 60rpx;
}

.special-wrapper {
  position: absolute;
  top: -70rpx;
  width: 160rpx;
  height: 150rpx;
  border-radius: 50%;
  background-color: #fff;
  text-align: center;
  box-sizing: border-box;
  left:7.5%;
  padding: 6rpx;
}

.special-wrapper-border {
  position: absolute;
  z-index: 888;
  top: -80rpx;
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  background-color: rgb(230, 231, 232);
  box-sizing: border-box;
  /* padding: 6rpx; */
  left: 52.0%;
  transform: translateX(-50%);
}

.special-wrapper .tabbar_icon {
  width: 144rpx;
  height: 110rpx;
  margin-left: -10rpx;
  margin-top: 20rpx;
}

.special-text-wrapper {
  width: 56rpx;
  height: 56rpx;
}